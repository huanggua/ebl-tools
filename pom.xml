				</exclusion>
			</exclusions>
		</dependency>
		<dependency>
			<groupId>com.wangyin</groupId>
			<artifactId>wangyin-commons</artifactId>
			<version>1.0.6</version>
			<exclusions>
				<exclusion>
					<artifactId>logback-classic</artifactId>
					<groupId>ch.qos.logback</groupId>
				</exclusion>
				<exclusion>
					<artifactId>logback-core</artifactId>
					<groupId>ch.qos.logback</groupId>
