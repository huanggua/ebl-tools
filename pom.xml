			<version>1.0.9</version>
		</dependency>
		<dependency>
			<groupId>com.wangyin.account</groupId>
			<artifactId>account-api-query</artifactId>
			<version>1.0.3</version>
		</dependency>
		<!-- dubbo -->
		<dependency>
			<groupId>com.alibaba</groupId>
			<artifactId>dubbo</artifactId>
			<version>2.4.10.1-wy</version>
			<exclusions>
				<exclusion>
					<groupId>org.springframework</groupId>
					<artifactId>spring</artifactId>
				</exclusion>
			</exclusions>
		</dependency>
		<dependency>
			<groupId>org.apache.zookeeper</groupId>
