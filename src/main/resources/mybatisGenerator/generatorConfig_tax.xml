<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
	<!--数据库驱动 -->
	<classPathEntry location="mysql-connector-java-5.0.8.jar" />
	<context id="DB2Tables" targetRuntime="MyBatis3">
		<commentGenerator>
			<property name="suppressDate" value="true" />
			<property name="suppressAllComments" value="true" />
		</commentGenerator>
		<!--数据库链接地址账号密码 -->
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://172.24.0.12:3306/license?useUnicode=true"
			userId="license" password="license">
		</jdbcConnection>
		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

		<!--生成Model类存放位置 -->
		<javaModelGenerator targetPackage="com.wangyin.ebl.po.tax"
			targetProject="D://workspace/ebl-tools/src/main/java">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>

		<!--生成映射文件存放位置 -->
		<sqlMapGenerator targetPackage="com.wangyin.ebl.mapper.tax"
			targetProject="D://workspace/ebl-tools/src/main/java">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>
		
		<!--生成dao层文件存放位置 -->
		<javaClientGenerator type="XMLMAPPER"
			targetPackage="com.wangyin.ebl.dao.tax" targetProject="D://workspace/ebl-tools/src/main/java">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>
				
		<!--生成对应表及类名 -->
		<!-- 企业税务信息 -->
		<table tableName="license_tax_server_info" domainObjectName="LicenseTaxServerInfo"
			enableCountByExample="false" enableUpdateByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			selectByExampleQueryId="false"></table>
		<table tableName="license_tax_info" domainObjectName="LicenseTaxInfo"
			enableCountByExample="false" enableUpdateByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			selectByExampleQueryId="false"></table>
		<table tableName="license_tax_rate_info" domainObjectName="LicenseTaxRateInfo"
			enableCountByExample="false" enableUpdateByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			selectByExampleQueryId="false"></table>
		<table tableName="license_dzf_contract"
			domainObjectName="LicenseDzfContact" enableCountByExample="false"
			enableUpdateByExample="false" enableDeleteByExample="false"
			enableSelectByExample="false" selectByExampleQueryId="false"></table>
		<table tableName="license_tax_pay"
			domainObjectName="LicenseTaxPay"
			enableCountByExample="false" enableUpdateByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			selectByExampleQueryId="false"></table>
		<table tableName="license_tax_pay_detail" domainObjectName="LicenseTaxPayDetail"
			enableCountByExample="false" enableUpdateByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			selectByExampleQueryId="false"></table>
		<table tableName="license_order_product"
			domainObjectName="LicenseOrderProduct" enableCountByExample="false"
			enableUpdateByExample="false" enableDeleteByExample="false"
			enableSelectByExample="false" selectByExampleQueryId="false"></table>
		<table tableName="license_tax_credential" domainObjectName="LicenseTaxCredential"
			enableCountByExample="false" enableUpdateByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			selectByExampleQueryId="false"></table>
		
	</context>
</generatorConfiguration>